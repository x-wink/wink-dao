{
    "name": "@xwink/dao",
    "version": "0.0.1",
    "keywords": [
        "dao",
        "nodejs",
        "mysql",
        "orm"
    ],
    "author": {
        "name": "向文可",
        "email": "1041367524@qq.com",
        "url": "https://github.com/x-wink"
    },
    "repository": {
        "url": "https://github.com/x-wink/wink-dao"
    },
    "private": false,
    "files": [
        "dist",
        "types/src"
    ],
    "type": "module",
    "main": "dist/index.mjs",
    "module": "dist/index.mjs",
    "types": "types/src/index.d.ts",
    "browser": "dist/index.js",
    "exports": {
        ".": {
            "types": "./types/src/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "scripts": {
        "test": "vitest run",
        "coverage": "vitest run --coverage",
        "build": "tsc && vite build",
        "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
        "release": "bumpp package.json --commit \"release: v%s\" --push --all --tag && pnpm run build && pnpm publish --access public",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "prettier": "prettier --write ./**/*.{vue,ts,tsx,js,jsx,css,less,json,md,html}",
        "prepare": "husky install"
    },
    "peerDependencies": {
        "@xwink/utils": "^1.0.0",
        "mysql2": "^3.6.2"
    },
    "dependencies": {
        "@xwink/sql-builder": "^1.0.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.4.1",
        "@commitlint/config-conventional": "^18.4.0",
        "@types/lodash": "^4.14.191",
        "@types/mysql": "^2.15.21",
        "@vitest/coverage-v8": "^0.34.6",
        "@xwink/eslint-config": "1.1.1-beta.1",
        "bumpp": "^9.2.0",
        "conventional-changelog-cli": "^4.1.0",
        "dotenv": "^16.0.3",
        "eslint": "^8.53.0",
        "husky": "^8.0.3",
        "lint-staged": "^15.1.0",
        "prettier": "^3.1.0",
        "rollup-plugin-visualizer": "^5.9.2",
        "ts-node": "^10.9.1",
        "typescript": "~5.2.2",
        "vite": "^5.0.0",
        "vitest": "1.0.0-beta.3"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,vue}": [
            "npm run lint"
        ],
        "*.{vue,ts,tsx,js,jsx,css,less,json,md,html}": [
            "npm run prettier"
        ]
    }
}
